{
  "name": "vaceate-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MTI",
  "bin": "dist/main.js",
  "scripts": {
    "prebuild": "node prebuild.js",
    "postbuild": "node postbuild.js",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:hot": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cli": "npx nestjs-command"
  },
  "dependencies": {
    "@bigbangjs/file-storage": "^1.0.3",
    "@bigbangjs/file-storage-filesystem": "^1.0.3",
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/sequelize": "^0.2.0",
    "dotenv": "^8.2.0",
    "http-status-codes": "^2.1.4",
    "i18next": "^19.8.4",
    "i18next-fs-backend": "^1.0.8",
    "i18next-http-middleware": "^3.1.0",
    "joi": "^17.3.0",
    "lodash": "^4.17.20",
    "log4js": "^6.3.0",
    "minimist": "^1.2.5",
    "mysql2": "^2.2.5",
    "nestjs-command": "^2.0.0",
    "randomcolor": "^0.6.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "sequelize": "^6.5.0",
    "sequelize-typescript": "^2.0.0",
    "sharp": "^0.28.1",
    "text-to-svg": "^3.1.5",
    "ulid": "^2.3.0",
    "umzug": "^3.0.0-beta.14",
    "uuid": "^8.3.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.3",
    "@commitlint/config-conventional": "^12.1.3",
    "@nestjs/cli": "7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp": "^4.0.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  }
}
